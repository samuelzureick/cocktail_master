      <owl:someValuesFrom rdf:resource="#Gomme"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#contains"/>
      <owl:someValuesFrom rdf:resource="#SimpleSyrup"/>
